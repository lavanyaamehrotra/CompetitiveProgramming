package Assignment1;
import java.util.Scanner;

public class palindrome_no {
    public static void main(String[] args) {
        Scanner sc=new Scanner(System.in);
        int x=sc.nextInt();
        int temp=x;
        int rev=0;
        while(x>0){
            int digit=x%10;
            rev=(rev*10)+digit;
            x=x/10;
        } 
        if(rev==temp){
            System.out.println("True");
        }else{
            System.out.println("False");
        }
    }
}
